{
      "version": 2,
      "name": "ecommerce-app-by-arpit",
      "builds": [
            { "src": "app.js", "use": "@vercel/node" }
      ],
      "routes": [
            { "src": "/(.*)", "dest": "/app.js" }
      ],
      "rewrites": [
            { "source": "/(.*)", "destination": "/" }
      ],
      "requireStack": [ "/vercel/path0/server/app.js", "/vercel/path0/server/server.js" ]
}